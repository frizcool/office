{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "aymanalhattami/filament-slim-scrollbar": "^2.0",
        "barryvdh/laravel-dompdf": "^2.2",
        "bezhansalleh/filament-shield": "^3.2",
        "eightynine/filament-excel-import": "^3.0",
        "filament/filament": "^3.2",
        "filament/notifications": "^3.2",
        "hugomyb/filament-error-mailer": "^3.1",
        "illuminate/notifications": "*",
        "jeffgreco13/filament-breezy": "^2.4",
        "joaopaulolndev/filament-pdf-viewer": "^1.0",
        "laravel/framework": "^11.9",
        "laravel/tinker": "^2.9",
        "malzariey/filament-daterangepicker-filter": "^2.8",
        "openai-php/laravel": "^0.10.1",
        "pxlrbt/filament-spotlight": "^1.2",
        "saade/filament-autograph": "^3.1",
        "shuvroroy/filament-spatie-laravel-backup": "^2.2",
        "stechstudio/filament-impersonate": "^3.9",
        "swisnl/filament-backgrounds": "^1.1",
        "tomatophp/filament-settings-hub": "^1.0",
        "tomatophp/filament-translations": "^1.0",
        "tomatophp/filament-users": "^1.0",
        "z3d0x/filament-logger": "^0.7.2"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.0.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
